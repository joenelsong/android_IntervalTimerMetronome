        android:angle="45"
        android:centerX="35%"
        android:startColor="#E8E8E8"
        android:centerColor="#5E0A0A"
        android:endColor="#333333"
        android:type="linear"
        />
    <padding
        android:left="5dip"
        android:top="5dip"
        android:right="5dip"
        android:bottom="5dip"
        />
